        {
            "name": "Python: Notebook",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/notebooks/analisis_ventas.ipynb",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}
